.sidebar.well#tree
	%ul
		-count=0
		- result["endpoints"].each do |e|
			-count=count+1
			%li
				%a.endpointActuator{:href => "#", :id => "endpointActuator#{count}"} #{e.name}
				%ul
					-method_count = 0
					-e["methods"].each do |m|
					-method_count=method_count+1
						%li 
							%a.methodActuator{:href => "#endpoint#{count}method#{method_count}"} #{m.MethodName}